{
    "name": "wongyip/laravel-renderable",
    "description": "Laravel model rendering simplified.",
    "homepage": "https://www.lib.polyu.edu.hk",
    "authors": [
        {
            "name": "Wongyip",
            "email": "wongyip@outlook.com",
            "homepage": "https://github.com/wongyip"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "*",
        "rcrowe/twigbridge": "*",
        "spatie/html-element": "*",
        "wongyip/phphelpers": "*"
    },
    "autoload": {
        "psr-4": {
            "Wongyip\\Laravel\\Renderable\\": "src",
            "Test\\": "test"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wongyip\\Laravel\\Renderable\\RenderableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
	"prefer-stable" : true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
