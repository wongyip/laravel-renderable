{
    "name": "wongyip/laravel-renderable",
    "description": "Render Eloquent model with ease.",
    "homepage": "https://github.com/wongyip/laravel-renderable",
    "authors": [
        {
            "name": "Wongyip",
            "email": "wongyip@outlook.com",
            "homepage": "https://github.com/wongyip"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "*",
        "rcrowe/twigbridge": "*",
        "wongyip/phphelpers": "*",
        "wongyip/html-tags": "*",
        "wongyip/html-beautify": "^1.0",
        "ezyang/htmlpurifier": "^4.17"
    },
    "autoload": {
        "psr-4": {
            "Wongyip\\Laravel\\Renderable\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wongyip\\Laravel\\Renderable\\RenderableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
	"prefer-stable" : true
}
